# Path to your SQL Server backup directory
BACKUP_PATH=D:\Program Files\Microsoft SQL Server\MSSQL16.MSSQLSERVER\MSSQL\Backup

# User account that needs read access to the latest backups
# Format: DOMAIN\Username or .\LocalUser
TARGET_USER=DOMAIN\Username

# Path to the 7-Zip executable
SEVEN_ZIP_PATH=C:\Program Files\7-Zip\7z.exe

# Number of latest zip files to keep (default: 5)
KEEP_LATEST_ZIPS=5 